            -- CreateBind
            function section:CreateBind(name, defaultKey, callback, saveId)
                local saveKey = saveId or name
                local currentKey = settings[saveKey] or defaultKey
                
                local BindZone = Instance.new("Frame")
                BindZone.Name = "BindZone"
                BindZone.Parent = sectionFrame
                BindZone.Active = true
                BindZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                BindZone.BackgroundTransparency = 1.000
                BindZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
                BindZone.BorderSizePixel = 0
                BindZone.Position = UDim2.new(0, 0, elementY, 0)
                BindZone.Size = UDim2.new(0, 195, 0, 30)

                local BindName = Instance.new("TextLabel")
                BindName.Name = "BindName"
                BindName.Parent = BindZone
                BindName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                BindName.BackgroundTransparency = 1.000
                BindName.BorderColor3 = Color3.fromRGB(0, 0, 0)
                BindName.BorderSizePixel = 0
                BindName.Position = UDim2.new(0.0516529568, 0, 0.233005777, 0)
                BindName.Size = UDim2.new(0, 18, 0, 16)
                BindName.Font = Enum.Font.SourceSansBold
                BindName.Text = name
                BindName.TextColor3 = Color3.fromRGB(255, 255, 255)
                BindName.TextSize = 13.000
                BindName.TextXAlignment = Enum.TextXAlignment.Left

                local Bind = Instance.new("TextButton")
                Bind.Name = "Bind"
                Bind.Parent = BindZone
                Bind.BackgroundColor3 = Color3.fromRGB(23, 23, 23)
                Bind.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Bind.BorderSizePixel = 0
                Bind.Position = UDim2.new(0.533333361, 0, 0.233005777, 0)
                Bind.Size = UDim2.new(0, 70, 0, 16)
                Bind.Font = Enum.Font.SourceSansBold
                Bind.Text = formatKeyName(currentKey.Name)
                Bind.TextColor3 = Color3.fromRGB(255, 255, 255)
                Bind.TextSize = 12.000
                Bind.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)

                local UICorner_12 = Instance.new("UICorner")
                UICorner_12.CornerRadius = UDim.new(0, 5)
                UICorner_12.Parent = Bind

                local binding = false
                Bind.MouseButton1Click:Connect(function()
                    binding = true
                    Bind.Text = "..."
                end)

                UserInputService.InputBegan:Connect(function(input, processed)
                    if binding and not processed and input.KeyCode ~= Enum.KeyCode.Unknown then
                        currentKey = input.KeyCode
                        settings[saveKey] = currentKey
                        saveSettings(config.FileName or "eschaton_settings.json", settings)
                        Bind.Text = formatKeyName(currentKey.Name)
                        binding = false
                        if callback then callback(currentKey) end
                    end
                end)

                elementY = elementY + 0.25
                updateSectionSize()
            end
            
